                        </div>

                        <div class="wrt-nine wow fadeInDown" data-wow-duration="1.5s" data-wow-delay="6.2s">
                            <div class="s-icon">
                                <p class="step">07
                                <span class="item-01">{{ paragraph.field_whyrooftopsolar.7.entity.bp_tab_section_title.value|raw  }}</span>
                                </p>
                            </div>
                            <p class="item-02">{{ paragraph.field_whyrooftopsolar.7.entity.bp_statistic_header.value|raw  }}</p>
                        </div>                   
                    </div>     
                </div>           
            </div> 
            <div class="process--mobile">
                
               
            </div>           
        
    {% endblock %}
    </div>
