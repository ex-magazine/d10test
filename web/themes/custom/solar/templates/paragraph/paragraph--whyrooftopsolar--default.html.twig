{%
  set classes = [
    'paragraph',  
    'paragraph--type--' ~ paragraph.bundle|clean_class,
    view_mode ? 'paragraph--view-mode--' ~ view_mode|clean_class,
    not paragraph.isPublished() ? 'paragraph--unpublished'
  ]
%}
{% block paragraph %}
    <div{{ attributes.addClass(classes) }}>
    {% block content %}         
            <div class="process--desktop">
                <div class="process--desktop-inner">
                    <div class="wrt-process-left">
                        <div class="wrt-one wow fadeIn" data-wow-duration="1.5s" data-wow-delay="0.8s">
                            <div class="s-icon">
                                <p class="step">01
                                <span class="item-01">{{ paragraph.field_whyrooftopsolar.0.entity.bp_tab_section_title.value|raw  }}</span>
                                </p>
                            </div>                            
                            <p class="item-02">{{ paragraph.field_whyrooftopsolar.0.entity.bp_statistic_header.value|raw  }}</p>
                        </div>

                        <div class="wrt-two wow fadeInDown" data-wow-duration="1.5s" data-wow-delay="1.2s">
                            <div class="s-icon">
                                <p class="step">02
                                <span class="item-01">{{ paragraph.field_whyrooftopsolar.1.entity.bp_tab_section_title.value|raw  }}</span>
                                </p>
                            </div>
                            <p class="item-02">{{ paragraph.field_whyrooftopsolar.1.entity.bp_statistic_header.value|raw  }}</p>
                        </div>

                        <div class="wrt-three wow fadeInDown" data-wow-duration="1.5s" data-wow-delay="2.2s">
                            <div class="s-icon">
                                <p class="step">03
                                <span class="item-01">{{ paragraph.field_whyrooftopsolar.2.entity.bp_tab_section_title.value|raw  }}</span>
                                </p>
                            </div>
                            <p class="item-02">{{ paragraph.field_whyrooftopsolar.2.entity.bp_statistic_header.value|raw  }}</p>
                        </div>
                        <div class="wrt-four wow fadeInDown" data-wow-duration="1.5s" data-wow-delay="3.2s">
                            <div class="s-icon">
                                <p class="step">04
                                <span class="item-01">{{ paragraph.field_whyrooftopsolar.3.entity.bp_tab_section_title.value|raw  }}</span>
                                </p>
                            </div>
                            <p class="item-02">{{ paragraph.field_whyrooftopsolar.3.entity.bp_statistic_header.value|raw  }}</p>
                        </div>
                    </div>
                    <div class="wrt-process-center">                   
                        <div class="wrt-six wow fadeInDown" data-wow-duration="1.5s" data-wow-delay=".2s">
                            <img src="{{ file_url(paragraph.field_whyrooftopsolar.4.entity.bp_card_image.entity.fileuri) }}" alt="{{ paragraph.field_whyrooftopsolar.4.entity.bp_card_image.alt }}" title="{{ paragraph.field_whyrooftopsolar.4.entity.bp_card_image.alt }}">
                            <div class="s-des">
                                <p class="item-03"><i class="far fa-check-circle"></i> {{ paragraph.field_whyrooftopsolar.4.entity.bp_tab_section_title.value|raw  }}</p>
                                <p class="item-04"><i class="far fa-check-circle"></i> {{ paragraph.field_whyrooftopsolar.4.entity.bp_statistic_header.value|raw  }}</p>
                            </div>
                        </div>
                    </div>
                    <div class="wrt-process-right">
                        <div class="wrt-seven wow fadeInDown" data-wow-duration="1.5s" data-wow-delay="4.2s">
                            <div class="s-icon">
                                <p class="step">05
                                <span class="item-01">{{ paragraph.field_whyrooftopsolar.5.entity.bp_tab_section_title.value|raw  }}</span>
                                </p>
                            </div>
                            <p class="item-02">{{ paragraph.field_whyrooftopsolar.5.entity.bp_statistic_header.value|raw  }}</p>
                        </div>

                        <div class="wrt-eight wow fadeInDown" data-wow-duration="1.5s" data-wow-delay="5.2s">
